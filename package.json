    "@tanstack/react-table": "^8.21.2",
    "@types/uuid": "^10.0.0",
    "axios": "^1.8.4",
    "debug": "^4.3.4",
    "framer-motion": "^11.0.3",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "next": "15.2.3",
