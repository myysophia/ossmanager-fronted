    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.5.0",
    "swr": "^2.3.3",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
